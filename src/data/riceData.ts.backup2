  id: string;
  name: string;
  slug: string;
  description: string;
  icon: string;
  image: string;
  sections: RiceSection[];
}

// Helper function to generate slug from name
const generateSlug = (name: string): string => {
  return name.toLowerCase().replace(/[^a-z0-9]+/g, '-').replace(/(^-|-$)/g, '');
};

